    <script type="text/javascript">
        $(document).ready(function () {
            cod = $("#cod").val();
            if (cod == 'tabla_area') {
                document.getElementById("tabArea").checked = true;
            } else if (cod == 'tabla_unidadMetrica') {
                document.getElementById("tabUnidadMetrica").checked = true;
            } else if (cod == 'tabla_edificio') {
                document.getElementById("tabEdificio").checked = true;
            } else if (cod == 'tabla_unidades') {
                document.getElementById("tabUnidad").checked = true;
            } else if (cod == 'tabla_statusSolicitud') {
                document.getElementById("tabSolicitud").checked = true;
            } else if (cod == 'tabla_prioridad') {
                document.getElementById("tabPrioridad").checked = true;
            } else if (cod == 'tabla_lugar') {
                document.getElementById("tabLugar").checked = true;
            } else if (cod == 'tabla_empleado') {
                document.getElementById("tabEmpleado").checked = true;
            } else if (cod == 'tabla_inventario') {
                document.getElementById("tabMateriales").checked = true;
            } else if (cod == 'tabla_supervisor') {
                document.getElementById("tabSupervisor").checked = true;
            } else {
                document.getElementById("tabArea").checked = true;

            }
        });


    </script>
    </html>

